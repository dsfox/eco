{
  "name": "Eco",
  "description": "Collapse equal tabs to single tab.",
  "version": "0.5.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon128.png"
  },
  "default_locale": "ru",
  "offline_enabled": true,
  "permissions": ["tabs"],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "options_page": "options.html"
}